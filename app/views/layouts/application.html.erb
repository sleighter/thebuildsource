<!DOCTYPE html>
<%= render :partial => "/shared/html_tag" %>
<head>
  <title>Thebuildsource</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= render :partial => "/shared/javascripts" %>
  <%= csrf_meta_tag %>
</head>
<body style="padding:40px;">
    <%= render :partial => "/shared/ie6check" if request.env['HTTP_USER_AGENT'] =~ /MSIE/ -%>
	<div class="container_16" id="body-container">
		<img id="corner-logo" class="grid_10" src="/images/MainLogo-Smaller.png"/>
		<div class="grid_6" id="main-menu">
		        <%= render :partial => "/shared/header" -%>
		</div>
		<div class="grid_16">
			<%= yield %>
		</div>
	</div>
</body>
</html>
